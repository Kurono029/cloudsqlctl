{
  "name": "cloudsqlctl",
  "version": "0.3.0",
  "description": "Windows-native CLI tool that installs, updates, and manages Google Cloud SQL Auth Proxy via gcloud CLI.",
  "main": "dist/cli.cjs",
  "type": "module",
  "bin": {
    "cloudsqlctl": "./dist/cli.cjs"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsup src/cli.ts --format cjs --minify --out-dir dist --no-splitting",
    "package": "npm run build && powershell -ExecutionPolicy Bypass -File tools/build-sea.ps1",
    "installer": "powershell -ExecutionPolicy Bypass -File tools/build-installer.ps1",
    "stage": "powershell -ExecutionPolicy Bypass -File tools/stage-artifacts.ps1",
    "lint": "eslint src",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --passWithNoTests",
    "prepare": "husky install"
  },
  "keywords": [
    "windows",
    "cli",
    "gcloud",
    "cloud-sql",
    "devops",
    "typescript",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "conf": "^15.0.2",
    "execa": "^9.6.1",
    "extract-zip": "2.0.1",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.15",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/extract-zip": "2.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^25.0.3",
    "eslint": "^9.0.0",
    "globals": "^16.5.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "postject": "1.0.0-alpha.6",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.0.0"
  }
}
