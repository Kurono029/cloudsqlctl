{
  "name": "cloudsqlctl",
  "version": "0.2.0",
  "description": "Windows-native CLI tool that installs, updates, and manages Google Cloud SQL Auth Proxy via gcloud CLI.",
  "main": "dist/cli.js",
  "type": "module",
  "bin": {
    "cloudsqlctl": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup src/cli.ts --format cjs --minify --out-dir dist --no-splitting",
    "package": "pkg dist/cli.js --targets node20-win-x64 --output bin/cloudsqlctl.exe",
    "zip": "powershell -Command \"Get-ChildItem -Path src, tests, tools, package.json, tsconfig.json, tsup.config.ts, jest.config.ts, .gitignore, eslint.config.mjs, LICENSE, *.md -ErrorAction SilentlyContinue | Compress-Archive -DestinationPath cloudsqlctl.zip -Force\"",
    "lint": "eslint src",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --passWithNoTests",
    "prepare": "husky install"
  },
  "keywords": [
    "windows",
    "cli",
    "gcloud",
    "cloud-sql",
    "devops",
    "typescript",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "conf": "^12.0.0",
    "execa": "^8.0.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.15",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "eslint": "^9.0.0",
    "globals": "^15.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "postject": "1.0.0-alpha.6",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.0.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
